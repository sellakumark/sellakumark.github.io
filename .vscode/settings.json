// Place your settings in this file to overwrite default and user settings.
{
    "eslint.format.enable": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "javascript",
        "typescript"
    ],
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "editor.insertSpaces": true
}